{
    "files.exclude": {
        "**/.bashrc": true,
        "**/.cache": true,
        "**/.config": true,
        "**/.jupyter": true,
        "**/.local": true,
        "**/.npm": true,
        "**/.yarn": true,
        "**/jupyter_vscode_proxy.egg-info": true,
        "**/jupyter_vscode_proxy": true,
        "**/.vscode": true,
        "**/.mamba": true,
        "**/.github": true,
        "**/.ipython": true,
        "**/.bash_logout": true,
        "**/.profile": true,
        "**/.jupyter-server-log.txt": true,
        "**/.env": true,
        "**/.flake8": true,
        "**/.gitignore": true,
        "**/.pre-commit-config.yaml": true,
        "**/LICENSE": true,
        "**/MANIFEST.in": true,
        "**/README.md": true,
        "**/environment.yml": true,
        "**/postBuild": true,
        "**/pyproject.toml": true
    },
    "workbench.colorTheme": "Default Dark Modern",
    "code-runner.showExecutionMessage": false,
    "code-runner.clearPreviousOutput": true
	"git.openRepositoryInParentFolders": "never"
}
